<script>
    function IsApproveClick() {
        if ($('#IsApprove').is(":checked") == true)
            $('#IsApplyforAdmission').attr("disabled", true).attr("checked", false);
        else
            $('#IsApplyforAdmission').attr("disabled", false);
    }
</script>