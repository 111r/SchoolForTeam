    <script>
        $("#TransportFacility").on('click', function () {
            var id = $(this).val();
            if (id == 0)
                return false;
            if (id == "yes")
                $("#busnumber").show(),
                $("#transportavailability").show();
         
            else
            $("#transportavailability").hide();
            
        });


        $("#btnverify").on('click', function () {
            validatestudentform();

        });

        $("#btnverifyfamily").on('click', function () {
            validateFamilydetailsform();

        });

        $("#btnverifyguardian").on('click', function () {
            $("#additionallink").tab('show');
        });

        $("#btnverifyadditional").on('click', function () {
            $("#pastschoollink").tab('show');
        });
        function validatestudentform() {
            var msg = "";
            var sname = $("#SName").val();
            var nationality = $("#Nationality").val();
            var sDOB = $("#DOB").val();
            var parentemail = $("#parentemail").val();
            var classid = $("#Class_Id").val();
            var transport = $("#TransportFacility").val();
            var Medium = $("#Medium").val();
            if (sname == "") {
                msg += "Name Cannot be Blank\r";
            }
            if (nationality == "") {
                msg += "Nationality Cannot be blank\r";
            }
            if (sDOB == "") {
                msg += "DOB Cannot be blank\r";
            }

            if (parentemail == "") {
                msg += "Email Cannot be blank\r";
            }
            if (classid == "" || classid == null) {
                msg += "Class Cannot be blank\r";
            }

            if (transport == "" || transport == null) {
                msg += "Tansport Option Cannot be blank\r";
            }
            if (Medium == "" || Medium == null) {
                msg += "Medium Option Cannot be blank\r";
            }

            if (msg != "") {
                alert("Please refer to the following error(s) :\r " + msg);
            } else {
                $("#familiylink").tab('show');
            };

        }
        function validateFamilydetailsform() {
            var msg = "";
            var fathername = $("#FatherName").val();
            var femail = $("#FEMail").val();
            var fatheroccupation = $("#FOccupation").val();
            var fathermobile = $("#FMobile").val();
