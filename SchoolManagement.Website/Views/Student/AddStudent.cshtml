            var motheroccupation = $("#MOccupation").val();
            var mothermobile = $("#MMobile").val();

            if (fathername == "") {
                msg += "FatherName Cannot be blank\r";
            }

            if (femail == "") {
                msg += "Father Email Cannot be Blank\r";
            }

            if (fatheroccupation == "") {
                msg += "Father Occupation Cannot be Blank\r";
            }

            if (fathermobile == "") {
                msg += "Father Mobile Cannot be Blank\r";
            }

            if (mothername == "") {
                msg += "Mother Name Cannot be Blank\r";
            }

            if (motheremail == "") {
                msg += "Mother Email Cannot be Blank\r";
            }

            if (motheroccupation == "") {
                msg += "Mother Occupation Cannot be Blank\r";
            }

            if (mothermobile == "") {
                msg += "Mother Mobile Cannot be Blank\r";
            }

            if (msg != "") {

